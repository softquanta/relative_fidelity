Cell[BoxData[{"\"\<To calculate Fcon, let \!\(\*SubscriptBox[\(\[Theta]\), \
\(1\)]\) \[Rule] \!\(\*SubscriptBox[\(\[Theta]\), \(2\)]\) in the output \
fidelity.\>\"", "\[IndentingNewLine]", 
 FractionBox[
  SqrtBox[
   RowBox[{"1", "+", 
    RowBox[{
     RowBox[{"Cos", "[", 
      RowBox[{"4", " ", "\[CapitalDelta]\[Theta]"}], "]"}], " ", 
     RowBox[{"Cos", "[", 
      RowBox[{"2", " ", 
       SubscriptBox["\[Theta]", "1"]}], "]"}], " ", 
     RowBox[{"Cos", "[", 
      RowBox[{"2", " ", 
       RowBox[{"(", 
        RowBox[{"\[CapitalDelta]\[Theta]", "+", 
         SubscriptBox["\[Theta]", "1"]}], ")"}]}], "]"}]}], "+", 
    SqrtBox[
     RowBox[{
      SuperscriptBox[
       RowBox[{"Sin", "[", 
        RowBox[{"2", " ", 
         SubscriptBox["\[Theta]", "1"]}], "]"}], "2"], " ", 
      SuperscriptBox[
       RowBox[{"Sin", "[", 
        RowBox[{"2", " ", 
         RowBox[{"(", 
          RowBox[{"\[CapitalDelta]\[Theta]", "+", 
           SubscriptBox["\[Theta]", "1"]}], ")"}]}], "]"}], "2"]}]]}]], 
  SqrtBox[
   "2"]], "\[IndentingNewLine]", "\"\<We impose 0 < \[CapitalDelta]\[Theta] < \
\!\(\*FractionBox[\(\[Pi]\), \(2\)]\) WLOG (since replacing \[CapitalDelta]\
\[Theta] with \[Pi] - \[CapitalDelta]\[Theta] and \!\(\*SubscriptBox[\(\
\[Theta]\), \(1\)]\) with \!\(\*FractionBox[\(\[Pi]\), \(2\)]\) - \
\!\(\*SubscriptBox[\(\[Theta]\), \(1\)]\) leaves the relative fidelity \
unchanged).\>\"", "\[IndentingNewLine]", "\"\<We can confine \
\!\(\*SubscriptBox[\(\[Theta]\), \(1\)]\) to 0 \[LessEqual] \
\!\(\*SubscriptBox[\(\[Theta]\), \(1\)]\) < \!\(\*FractionBox[\(\[Pi]\), \
\(2\)]\) WLOG (since replacing \!\(\*SubscriptBox[\(\[Theta]\), \(1\)]\) with \
\!\(\*FractionBox[\(\[Pi]\), \(2\)]\) + \!\(\*SubscriptBox[\(\[Theta]\), \
\(1\)]\) leaves the relative fidelity unchanged).\>\"", \
"\[IndentingNewLine]", "\"\<In this range, Sin[2\!\(\*SubscriptBox[\(\[Theta]\
\), \(1\)]\)] \[GreaterEqual] 0.\>\"", "\[IndentingNewLine]", "\"\<We now \
have two regimes: \[CapitalDelta]\[Theta] + \!\(\*SubscriptBox[\(\[Theta]\), \
\(1\)]\) < \!\(\*FractionBox[\(\[Pi]\), \(2\)]\) and \[CapitalDelta]\[Theta] \
+ \!\(\*SubscriptBox[\(\[Theta]\), \(1\)]\) \[GreaterEqual] \
\!\(\*FractionBox[\(\[Pi]\), \(2\)]\).\>\"", "\[IndentingNewLine]", \
"\"\<Regime 1:\>\"", "\[IndentingNewLine]", "\"\<Output fidelity (after \
simplification)\>\"", "\[IndentingNewLine]", 
 FractionBox[
  SqrtBox[
   RowBox[{"1", "+", 
    SuperscriptBox[
     RowBox[{"Cos", "[", 
      RowBox[{"2", " ", "\[CapitalDelta]\[Theta]"}], "]"}], "3"], "-", 
    RowBox[{
     RowBox[{"Cos", "[", 
      RowBox[{"2", " ", 
       RowBox[{"(", 
        RowBox[{"\[CapitalDelta]\[Theta]", "+", 
         RowBox[{"2", " ", 
          SubscriptBox["\[Theta]", "1"]}]}], ")"}]}], "]"}], " ", 
     SuperscriptBox[
      RowBox[{"Sin", "[", 
       RowBox[{"2", " ", "\[CapitalDelta]\[Theta]"}], "]"}], "2"]}]}]], 
  SqrtBox[
   "2"]], "\[IndentingNewLine]", "\"\<Minimisation is equivalent to \
maximising Cos[2(\[CapitalDelta]\[Theta] + 2\!\(\*SubscriptBox[\(\[Theta]\), \
\(1\)]\))].\>\"", "\[IndentingNewLine]", "\"\<Minimized (under the contraints \
on \[CapitalDelta]\[Theta] and \!\(\*SubscriptBox[\(\[Theta]\), \(1\)]\)) by \
setting \!\(\*SubscriptBox[\(\[Theta]\), \(1\)]\) \[Rule] 0.\>\"", "\
\[IndentingNewLine]", "\"\<Minimum value\>\"", "\[IndentingNewLine]", 
 RowBox[{"FullSimplify", "[", 
  RowBox[{
   FractionBox[
    SqrtBox[
     RowBox[{"1", "+", 
      SuperscriptBox[
       RowBox[{"Cos", "[", 
        RowBox[{"2", " ", "\[CapitalDelta]\[Theta]"}], "]"}], "3"], "-", 
      RowBox[{
       RowBox[{"Cos", "[", 
        RowBox[{"2", " ", 
         RowBox[{"(", 
          RowBox[{"\[CapitalDelta]\[Theta]", "+", 
           RowBox[{"2", " ", 
            SubscriptBox["\[Theta]", "1"]}]}], ")"}]}], "]"}], " ", 
       SuperscriptBox[
        RowBox[{"Sin", "[", 
         RowBox[{"2", " ", "\[CapitalDelta]\[Theta]"}], "]"}], "2"]}]}]], 
    SqrtBox["2"]], "/.", 
   RowBox[{
    SubscriptBox["\[Theta]", "1"], " ", "\[Rule]", " ", "0"}]}], 
  "]"}], "\[IndentingNewLine]", "\"\<Regime 2:\>\"", "\[IndentingNewLine]", "\
\"\<Output fidelity (after simplification)\>\"", "\[IndentingNewLine]", 
 FractionBox[
  SqrtBox[
   RowBox[{"1", "+", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"Cos", "[", 
       RowBox[{"2", " ", "\[CapitalDelta]\[Theta]"}], "]"}], "2"], " ", 
     RowBox[{"Cos", "[", 
      RowBox[{"2", 
       RowBox[{"(", 
        RowBox[{"\[CapitalDelta]\[Theta]", "+", 
         RowBox[{"2", 
          SubscriptBox["\[Theta]", "1"]}]}], ")"}]}], "]"}]}], "-", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"Sin", "[", 
       RowBox[{"2", " ", "\[CapitalDelta]\[Theta]"}], "]"}], "2"], 
     RowBox[{"Cos", "[", 
      RowBox[{"2", "\[CapitalDelta]\[Theta]"}], "]"}]}]}]], 
  SqrtBox[
   "2"]], "\[IndentingNewLine]", "\"\<Minimisation is equivalent to \
minimising Cos[2(\[CapitalDelta]\[Theta] + 2\!\(\*SubscriptBox[\(\[Theta]\), \
\(1\)]\))].\>\"", "\[IndentingNewLine]", "\"\<Minimized (under the contraints \
