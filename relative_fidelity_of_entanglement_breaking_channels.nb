Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
    "Function", " ", "for", " ", "the", " ", "minimum", " ", "relative", " ", 
     "fidelity", " ", "with", " ", "f"}], " ", ">", " ", 
    RowBox[{
    "the", " ", "required", " ", "input", " ", "fidelity", " ", "to", " ", 
     "achieve", " ", 
     SubscriptBox["F", 
      RowBox[{"R", ",", "min"}]], 
     RowBox[{"(", "0", ")"}]}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"FR", "[", 
     RowBox[{"f_", ",", "\[CapitalDelta]\[Theta]_"}], "]"}], ":=", 
    RowBox[{"Min", "[", 
     RowBox[{
      FractionBox[
       SqrtBox[
        RowBox[{
         FractionBox["1", 
          SuperscriptBox["f", "2"]], "+", 
         RowBox[{
          RowBox[{"Cos", "[", 
           RowBox[{"4", " ", "\[CapitalDelta]\[Theta]"}], "]"}], " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"2", "-", 
               FractionBox["1", 
                SuperscriptBox["f", "2"]]}], ")"}], " ", 
             RowBox[{"Cos", "[", 
              RowBox[{"2", " ", "\[CapitalDelta]\[Theta]"}], "]"}]}], "-", 
            RowBox[{"2", " ", 
             SqrtBox[
              RowBox[{
               RowBox[{"-", "1"}], "+", 
               FractionBox["1", 
                SuperscriptBox["f", "2"]]}]], " ", 
             RowBox[{"Sin", "[", 
              RowBox[{"2", " ", "\[CapitalDelta]\[Theta]"}], "]"}]}]}], 
           ")"}]}]}]], 
       SqrtBox["2"]], ",", 
      FractionBox[
       SqrtBox[
        RowBox[{
         FractionBox["1", 
          SuperscriptBox["f", "2"]], "+", 
         RowBox[{
          RowBox[{"Cos", "[", 
           RowBox[{"4", " ", "\[CapitalDelta]\[Theta]"}], "]"}], " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"2", "-", 
               FractionBox["1", 
                SuperscriptBox["f", "2"]]}], ")"}], " ", 
             RowBox[{"Cos", "[", 
              RowBox[{"2", " ", "\[CapitalDelta]\[Theta]"}], "]"}]}], "+", 
            RowBox[{"2", " ", 
             SqrtBox[
              RowBox[{
               RowBox[{"-", "1"}], "+", 
               FractionBox["1", 
                SuperscriptBox["f", "2"]]}]], " ", 
             RowBox[{"Sin", "[", 
              RowBox[{"2", " ", "\[CapitalDelta]\[Theta]"}], "]"}]}]}], 
           ")"}]}]}]], 
       SqrtBox["2"]]}], "]"}]}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Function", " ", "for", " ", "the", " ", "required", " ", "input", " ", 
     "fidelity", " ", "to", " ", "achieve", " ", 
     SubscriptBox["F", 
      RowBox[{"R", ",", "min"}]], 
     RowBox[{"(", "0", ")"}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"ReqInFid", "[", "\[CapitalDelta]\[Theta]_", "]"}], ":=", 
    FractionBox[
     RowBox[{"1", "-", 
      RowBox[{
       RowBox[{"Cos", "[", 
        RowBox[{"2", " ", "\[CapitalDelta]\[Theta]"}], "]"}], " ", 
       RowBox[{"Cos", "[", 
        RowBox[{"4", " ", "\[CapitalDelta]\[Theta]"}], "]"}]}]}], 
     RowBox[{
      SqrtBox["2"], " ", 
      RowBox[{"Abs", "[", 
       RowBox[{"Sin", "[", "\[CapitalDelta]\[Theta]", "]"}], "]"}], " ", 
      SqrtBox[
       RowBox[{"2", "+", 
        RowBox[{"Cos", "[", 
         RowBox[{"2", " ", "\[CapitalDelta]\[Theta]"}], "]"}], "-", 
        RowBox[{"Cos", "[", 
         RowBox[{"6", " ", "\[CapitalDelta]\[Theta]"}], "]"}]}]]}]]}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Function", " ", "for", " ", 
     SubscriptBox["F", 
      RowBox[{"R", ",", "min"}]], 
     RowBox[{"(", "0", ")"}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"MinRelFid", "[", "\[CapitalDelta]\[Theta]_", "]"}], ":=", 
    SqrtBox[
     FractionBox[
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Cos", "[", "\[CapitalDelta]\[Theta]", "]"}], "+", 
         RowBox[{"Cos", "[", 
          RowBox[{"3", " ", "\[CapitalDelta]\[Theta]"}], "]"}]}], ")"}], "2"], 
      RowBox[{"2", "+", 
       RowBox[{"2", " ", 
        RowBox[{"Cos", "[", 
         RowBox[{"2", " ", "\[CapitalDelta]\[Theta]"}], "]"}]}], "+", 
       RowBox[{"Cos", "[", 
        RowBox[{"4", " ", "\[CapitalDelta]\[Theta]"}], 
        "]"}]}]]]}]}]}]], "Input",
