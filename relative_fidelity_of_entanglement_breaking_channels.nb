Cell[BoxData[{"\"\<Finally, we demonstrate that there exists an adaptive \
strategy that beats any non-adaptive strategy for certain values of \
\[CapitalDelta]\[Theta].\>\"", "\[IndentingNewLine]", "\"\<We consider a \
2-use protocol. The input for the first use is the state |0>. The Pauli-X \
operator is applied to the output and the result is the input for the second \
use.\>\"", "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"adaptiveOut1", "=", 
   RowBox[{"FullSimplify", "[", 
    RowBox[{
     RowBox[{"2", 
      RowBox[{"TraceFirstMode", "[", 
       RowBox[{"ChoiC1", ".", 
        RowBox[{"KroneckerProduct", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"PauliMatrix", "[", "1", "]"}], ".", 
           RowBox[{"(", 
            RowBox[{"out1", "/.", 
             RowBox[{
              SubscriptBox["\[Theta]", "1"], "\[Rule]", "0"}]}], ")"}], ".", 
           RowBox[{"ConjugateTranspose", "[", 
            RowBox[{"PauliMatrix", "[", "1", "]"}], "]"}]}], ",", 
          RowBox[{"IdentityMatrix", "[", "2", "]"}]}], "]"}]}], "]"}]}], ",", 
     "conds"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"adaptiveOut2", "=", 
   RowBox[{"FullSimplify", "[", 
    RowBox[{
     RowBox[{"2", 
      RowBox[{"TraceFirstMode", "[", 
       RowBox[{"ChoiC2", ".", 
        RowBox[{"KroneckerProduct", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"PauliMatrix", "[", "1", "]"}], ".", 
           RowBox[{"(", 
            RowBox[{"out2", "/.", 
             RowBox[{
              SubscriptBox["\[Theta]", "2"], "\[Rule]", "0"}]}], ")"}], ".", 
           RowBox[{"ConjugateTranspose", "[", 
            RowBox[{"PauliMatrix", "[", "1", "]"}], "]"}]}], ",", 
          RowBox[{"IdentityMatrix", "[", "2", "]"}]}], "]"}]}], "]"}]}], ",", 
     "conds"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", "\"\<Output fidelity\>\"", "\
\[IndentingNewLine]", 
 RowBox[{"adaptiveFid", "=", 
  RowBox[{"FullSimplify", "[", 
   RowBox[{
    RowBox[{"Fidelity", "[", 
     RowBox[{"adaptiveOut1", ",", "adaptiveOut2"}], "]"}], ",", "conds"}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{"Plot", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Fcon", "^", "2"}], ",", "adaptiveFid"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"\[CapitalDelta]\[Theta]", ",", "0", ",", 
     RowBox[{"Pi", "/", "2"}]}], "}"}], ",", 
   RowBox[{"AxesLabel", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
     "\"\<\[CapitalDelta]\[Theta]\>\"", ",", 
      "\"\<\!\(\*SubscriptBox[\(F\), \(out\)]\)\>\""}], "}"}]}], ",", 
   RowBox[{"PlotLegends", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
     "\"\<\!\(\*SuperscriptBox[SubscriptBox[\(F\), \(con\)], \(2\)]\)\>\"", 
      ",", "\"\<adaptive strategy\>\""}], "}"}]}]}], 
  "]"}], "\[IndentingNewLine]", "\"\<Since the adaptive strategy has a lower \
fidelity than \!\(\*SuperscriptBox[SubscriptBox[\(F\), \(con\)], \(2\)]\), \
for certain values of \[CapitalDelta]\[Theta], adaptivity is shown to be of \
benefit for those values of \[CapitalDelta]\[Theta].\>\""}], "Input",
